{
  "name": "@bufbuild/protovalidate-testing",
  "private": true,
  "bin": {
    "protovalidate-conformance": "bin/protovalidate-conformance.cjs"
  },
  "scripts": {
    "install-protovalidate-conformance": "node scripts/install-protovalidate-conformance.js",
    "generate": "buf generate buf.build/bufbuild/protovalidate-testing:v0.9.0",
    "postgenerate": "license-header src/gen",
    "test": "protovalidate-conformance --expected_failures=expected-failures.yaml -- tsx src/executor.ts",
    "format": "prettier --write --ignore-unknown '.' '!dist' '!src/gen'",
    "lint": "eslint .",
    "license-header": "license-header --ignore 'src/gen/**'"
  },
  "type": "module",
  "sideEffects": false,
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "^2.2.3",
    "@bufbuild/protobuf": "^2.2.3",
    "@bufbuild/protovalidate": "^0.0.1"
  }
}
