    "lint": "biome lint --error-on-warnings",
    "license-header": "license-header"
  },
  "type": "module",
  "sideEffects": false,
  "devDependencies": {
    "@bufbuild/protobuf": "^2.5.0",
    "@bufbuild/protovalidate": "^0.1.1"
  }
}
